Project Thread is
  for Source_Dirs use ("src", "src/tests");
  for Object_Dir use "bin";
  for Exec_Dir use ".";
  for Main use ("test.adb", "main.adb");
  package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnato", "-gnatwa", "-gnatQ", "-gnat12");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-g", "-gnat12");
   end Builder;
end Thread;
